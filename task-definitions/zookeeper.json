[
  {
    "name": "zookeeper",
    "image": "confluentinc/cp-zookeeper:3.1.1",
    "cpu": 10,
    "memory": 512,
    "essential": true,
    "environment": [
      {
        "name": "ZOOKEEPER_CLIENT_PORT",
        "value": "2181"
      }
    ],
    "portMappings": [
      {
        "containerPort": 2181,
        "hostPort": 2181
      }
    ]
  }
]
